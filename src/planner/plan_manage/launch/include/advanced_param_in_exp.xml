        <arg name="point2_z" value="1.0" />
        <arg name="point3_x" value="0.0" />
        <arg name="point3_y" value="-3.3" />
        <arg name="point3_z" value="1.0" />
        <arg name="point4_x" value="0.0" />
        <arg name="point4_y" value="0.0" />
        <arg name="point4_z" value="1.0" />
        
        <arg name="point5_x" value="4.2" />
        <arg name="point5_y" value="0.0" />
        <arg name="point5_z" value="1.0" />
        <arg name="point6_x" value="4.2" />
        <arg name="point6_y" value="-3.3" />
        <arg name="point6_z" value="1.0" />
        <arg name="point7_x" value="0.0" />
        <arg name="point7_y" value="-3.3" />
        <arg name="point7_z" value="1.0" />
        <arg name="point8_x" value="0.0" />
        <arg name="point8_y" value="0.0" />
        <arg name="point8_z" value="1.0" />  

        <arg name="point9_x" value="4.2" />
        <arg name="point9_y" value="0.0" />
        <arg name="point9_z" value="1.0" />
        <arg name="point10_x" value="4.2" />
        <arg name="point10_y" value="-3.3" />
        <arg name="point10_z" value="1.0" />
        <arg name="point11_x" value="0.0" />
        <arg name="point11_y" value="-3.3" />
        <arg name="point11_z" value="1.0" />
        <arg name="point12_x" value="0.0" />
        <arg name="point12_y" value="0.0" />
        <arg name="point12_z" value="1.0" />

        <arg name="point13_x" value="4.2" />
        <arg name="point13_y" value="0.0" />
        <arg name="point13_z" value="1.0" />
        <arg name="point14_x" value="4.2" />
        <arg name="point14_y" value="-3.3" />
        <arg name="point14_z" value="1.0" />
        <arg name="point15_x" value="0.0" />
        <arg name="point15_y" value="-3.3" />
        <arg name="point15_z" value="1.0" />
        <arg name="point16_x" value="0.0" />
        <arg name="point16_y" value="0.0" />
        <arg name="point16_z" value="1.0" />
    </include>

</launch>
