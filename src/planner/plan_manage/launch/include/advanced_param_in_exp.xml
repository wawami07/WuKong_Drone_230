  <arg name="point3_y"/>
  <arg name="point3_z"/>
  <arg name="point4_x"/>
  <arg name="point4_y"/>
  <arg name="point4_z"/>
  <arg name="point5_x"/>
  <arg name="point5_y"/>
  <arg name="point5_z"/>
  <arg name="point6_x"/>
  <arg name="point6_y"/>
  <arg name="point6_z"/>

  <arg name="point7_x"/>
  <arg name="point7_y"/>
  <arg name="point7_z"/>
  <arg name="point8_x"/>
  <arg name="point8_y"/>
  <arg name="point8_z"/>
